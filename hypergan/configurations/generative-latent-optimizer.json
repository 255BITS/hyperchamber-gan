{
  "description":"Optimizing the Latent Space of Generative Networks",
  "publication":"https://arxiv.org/pdf/1707.05776.pdf",
  "discriminator": 
  {
    "activation": "prelu",
    "distance": "l1_distance",
    "class": "class:hypergan.discriminators.pyramid_discriminator.PyramidDiscriminator",
    "block": "function:hypergan.discriminators.common.standard_block",
    "extra_layers": 0,
    "extra_layers_reduction": 2,
    "fc_layer_size": 300,
    "fc_layers": 0,
    "final_activation": "tanh",
    "initial_depth": 64,
    "depth_increase": 64,
    "layers": 5,
    "orthogonal_initializer_gain": 1.0595959595959596,
    "resize": null
  },

  "generator": {
    "class": "function:hypergan.generators.resize_conv_generator.ResizeConvGenerator",
    "activation": "prelu",
    "final_depth": 64,
    "depth_increase": 64,
    "skip_linear": true,
    "concat_linear": 32,
    "concat_linear_filters": 32,
    "final_activation": "tanh"
  },

  "encoder":
  {
    "class": "function:hypergan.encoders.uniform_encoder.UniformEncoder",
    "projections": [
      "function:hypergan.encoders.uniform_encoder.identity"
    ],
    "min": -1,
    "max": 1,
    "z": 100
  },
  "loss":
  {
    "class": "function:hypergan.losses.least_squares_loss.LeastSquaresLoss",
    "reduce": "reduce_mean",
    "labels": [0,1,1]
  },
  "trainer": {
    "class": "class:hypergan.trainers.alternating_trainer.AlternatingTrainer",
    "d_beta1": 0.5,
    "d_learn_rate": 0.0001,
    "d_trainer": "class:tensorflow.python.training.adam.AdamOptimizer",
    "g_beta1": 0.5,
    "g_learn_rate": 0.0001,
    "g_trainer": "class:tensorflow.python.training.adam.AdamOptimizer"
  },
  "z_discriminator": 
  {
    "activation": "selu",
    "distance": "l2_distance",
    "class": "class:hypergan.discriminators.pyramid_discriminator.PyramidDiscriminator",
    "block": "function:hypergan.discriminators.common.standard_block",
    "encoder_fc_layers": 0,
    "extra_layers": 3,
    "extra_layers_reduction": 1,
    "fc_layer_size": 300,
    "final_activation": "tanh",
    "fc_layers": 1,
    "initial_depth": 32,
    "depth_increase": 32,
    "layers": 0,
    "orthogonal_initializer_gain": 1.0595959595959596,
    "resize": null
  },

  "class": "class:hypergan.gans.generative_latent_optimizer.GenerativeLatentOptimizer",
  "sampler_assign": true,
  "type": "reconstruction"
}
